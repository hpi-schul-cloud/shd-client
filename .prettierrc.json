{
  "$schema": "https://json.schemastore.org/prettierrc",
  "printWidth": 80,
  "useTabs": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "never",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf",
  "semi": true,
  "singleQuote": false
}